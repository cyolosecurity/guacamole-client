  },
  "homepage": "https://github.com/cyolosecurity/guacamole-client#readme",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "npm-run-all": "^4.1.5"
  }
